{
  "version": 2,
  "builds": [
    {
      "src": "server/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "server/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "BASE_URL": "https://phone-store-backend.vercel.app",
    "WEBSOCKET_URL": "wss://phone-store-backend.vercel.app"
  }
}